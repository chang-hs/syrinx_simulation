  pmid = {22986837},
  keywords = {Cervical Vertebrae,Disease-Free Survival,Epidemiologic Studies,Humans,Intervertebral Disc Degeneration,Lumbar Vertebrae,Postoperative Complications,prevalence,Publication Bias,Research Design,Sensitivity and Specificity,Spinal Fusion,Stress; Mechanical,Total Disc Replacement},
