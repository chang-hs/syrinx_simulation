@article{baba1996microsurgical,
  title = {Microsurgical Nerve Root Canal Widening without Fusion for Lumbosacral Intervertebral Foraminal Stenosis: Technical Notes and Early Results},
  shorttitle = {Microsurgical Nerve Root Canal Widening without Fusion for Lumbosacral Intervertebral Foraminal Stenosis},
