}

@article{stamates2018magnetic,
  title = {Magnetic Resonance Imaging in the Prone Position and the Diagnosis of Tethered Spinal Cord},
  author = {Stamates, Melissa M. and Frim, David M. and Yang, Carina W. and Katzman, Gregory L. and Ali, Saad},
  year = {2018},
