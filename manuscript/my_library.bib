@article{vlak2011prevalence,
  title = {Prevalence of Unruptured Intracranial Aneurysms, with Emphasis on Sex, Age, Comorbidity, Country, and Time Period: A Systematic Review and Meta-Analysis},
  shorttitle = {Prevalence of Unruptured Intracranial Aneurysms, with Emphasis on Sex, Age, Comorbidity, Country, and Time Period},
  author = {Vlak, Monique Hm and Algra, Ale and Brandenburg, Raya and Rinkel, Gabri{\"e}l Je},
  year = {2011},
  month = jul,
  journal = {The Lancet. Neurology},
  volume = {10},
  number = {7},
  pages = {626--636},
  issn = {1474-4465},
  doi = {10.1016/S1474-4422(11)70109-0},
  abstract = {BACKGROUND: Unruptured intracranial aneurysms (UIAs) are increasingly detected and are an important health-care burden. We aimed to assess the prevalence of UIAs according to family history, comorbidity, sex, age, country, and time period. METHODS: Through searches of PubMed, Embase, and Web of Science we updated our 1998 systematic review up to March, 2011. We calculated prevalences and prevalence ratios (PRs) with random-effects binomial meta-analysis. We assessed time trends with year of study as a continuous variable. FINDINGS: We included 68 studies, which reported on 83 study populations and 1450 UIAs in 94\hphantom{,}912 patients from 21 countries. The overall prevalence was estimated as 3{$\cdot$}2\% (95\% CI 1{$\cdot$}9-5{$\cdot$}2) in a population without comorbidity, with a mean age of 50 years, and consisting of 50\% men. Compared with populations without the comorbidity, PRs were 6{$\cdot$}9 (95\% CI 3{$\cdot$}5-14) for autosomal dominant polycystic kidney disease (ADPKD), 3{$\cdot$}4 (1{$\cdot$}9-5{$\cdot$}9) for a positive family history of intracranial aneurysm of subarachnoid haemorrhage, 3{$\cdot$}6 (0{$\cdot$}4-30) for brain tumour, 2{$\cdot$}0 (0{$\cdot$}9-4{$\cdot$}6) for pituitary adenoma, and 1{$\cdot$}7 (0{$\cdot$}9-3{$\cdot$}0) for atherosclerosis. The PR for women compared with men was 1{$\cdot$}61 (1{$\cdot$}02-2{$\cdot$}54), with a ratio of 2{$\cdot$}2 (1{$\cdot$}3-3{$\cdot$}6) in study populations with a mean age of more than 50 years. Compared with patients older than 80 years, we found no differences by age, except for patients younger than 30 years (0{$\cdot$}01, 0{$\cdot$}00-0{$\cdot$}12). Compared with the USA, PRs were similar for other countries, including Japan (0{$\cdot$}8, 0{$\cdot$}4-1{$\cdot$}7) and Finland (1{$\cdot$}0, 0{$\cdot$}4-2{$\cdot$}4). There was no statistically significant time trend. INTERPRETATION: The prevalence of UIAs is higher in patients with ADPKD or a positive family history of intracranial aneurysm of subarachnoid haemorrhage than in people without comorbidity. In Finland and Japan, the higher incidence of subarachnoid haemorrhage is not explained by a higher prevalence of UIAs, implicating higher risks of rupture. FUNDING: Julius Centre for Health Sciences and Primary Care and Department of Neurology and Neurosurgery, University Medical Centre, Utrecht.},
  langid = {english},
  pmid = {21641282},
  keywords = {Adult,Age Factors,Comorbidity,Female,Humans,Intracranial Aneurysm,Male,prevalence,Risk Factors,Sex Factors},
  file = {/home/chang/Dropbox/Zotero_pdf/vlak2011prevalence.pdf}
}

@article{voulgaris2010posterior,
  title = {Posterior Approach to Ventrally Located Spinal Meningiomas},
  author = {Voulgaris, Spyridon and Alexiou, George A. and Mihos, Evaggelos and Karagiorgiadis, Dimitrios and Zigouris, Andreas and Fotakopoulos, George and Drosos, Dimitrios and Pahaturidis, Dimitrios},
  year = {2010},
  month = jul,
  journal = {European Spine Journal: Official Publication of the European Spine Society, the European Spinal Deformity Society, and the European Section of the Cervical Spine Research Society},
  volume = {19},
  number = {7},
  pages = {1195--1199},
  issn = {1432-0932},
  doi = {10.1007/s00586-010-1295-z},
  abstract = {For the resection of anteriorly located meningiomas, various approaches have been used. Posterior approach is less invasive and demanding; however, it has been associated with increased risk of spinal cord injury. We evaluated ten consecutive patients that underwent surgery for spinal meningiomas. All patients were preoperative assessed by neurological examination, computed tomography and magnetic resonance imaging. All tumors were ventrally located and removed via a posterior approach. Transcranial motor-evoked potentials (TcMEPs), somatosensory-evoked potential (SSEP) and free running electromyography (EMG) were monitored intraoperative. Postoperative all patients had regular follow-up examinations. There were four males and six females. The mean age was 68.2 years (range 39-82 years). In nine out of ten cases, the tumor was located in the thoracic spine. A case of a lumbar meningioma was recorded. The most common presenting symptom was motor and sensory deficits and unsteady gait, whereas no patient presented with paraplegia. All meningiomas were operated using a microsurgical technique via a posterior approach. During the operation, free running EMG monitoring prompted a surgical alert in case of irritation, whereas TcMEP and SSEP amplitudes remained unchanged. Histopathology revealed the presence of typical (World Health Organisation grade I) meningiomas. The mean Ki-67/MIB-1 index was 2.75\% (range 0.5-7). None of our patients sustained a transient or permanent motor deficit. After a mean follow-up period of 26 months (range 56-16 months), no tumor recurrence and no instability were found. Posterior approach for anteriorly located meningiomas is a safe procedure with the use of intraoperative monitoring, less invasive and well-tolerated especially in older patients. Complete tumor excision can be performed with satisfactory results.},
  langid = {english},
  pmcid = {PMC2900010},
  pmid = {20127494},
  keywords = {Adult,Aged,Aged; 80 and over,Electromyography,Evoked Potentials; Motor,Evoked Potentials; Somatosensory,Female,Humans,Lumbar Vertebrae,Male,Meningeal Neoplasms,Meningioma,Middle Aged,Monitoring; Intraoperative,Neurologic Examination,Orthopedic Procedures,Spinal Neoplasms,Thoracic Vertebrae,Treatment Outcome},
  file = {/home/chang/Dropbox/Zotero_pdf/voulgaris2010posterior.pdf}
}

