@article{chang2014dorsal,
  title = {Dorsal Spinal Arachnoid Web Diagnosed with the Quantitative Measurement of Cerebrospinal Fluid Flow on Magnetic Resonance Imaging},
  author = {Chang, Han Soo and Nagai, Atsushi and Oya, Soichi and Matsui, Toru},
  year = {2014},
  month = feb,
  journal = {Journal of Neurosurgery. Spine},
  volume = {20},
  number = {2},
  pages = {227--233},
  issn = {1547-5646},
  doi = {10.3171/2013.10.SPINE13395},
  abstract = {An arachnoid web is an abnormal formation of the arachnoid membrane in the spinal subarachnoid space that blocks CSF flow and causes syringomyelia. Although the precise mechanism of syrinx formation is unknown, dissection of the arachnoid web shrinks the syrinx and improves symptoms. Precisely determining the location of the arachnoid web is difficult preoperatively, however, because the fine structure generally cannot be visualized in usual MRI sequences. In this report the authors describe 2 cases of arachnoid web in which the web was preoperatively identified using quantitative CSF flow analysis of MRI. By analyzing cardiac-gated phase-contrast cine-mode MRI in multiple axial planes, the authors precisely localized the obstruction of CSF flow on the dorsal side of the spinal cord in both patients. This technique also revealed a 1-way valve-like function of the arachnoid webs. Imaging led to the early diagnosis of myelopathy related to the derangement of CSF flow and allowed the authors to successfully excise the webs through limited surgical exposure.},
  langid = {english},
  pmid = {24313674},
  keywords = {Adult,Arachnoid,Female,Humans,Magnetic Resonance Imaging; Cine,Middle Aged,Subarachnoid Space,Syringomyelia,Treatment Outcome}
}

