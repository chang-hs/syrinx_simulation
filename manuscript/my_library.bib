  pmid = {17545903},
  keywords = {Adult,Aged,Bone Screws,Decompression; Surgical,Disability Evaluation,Female,Follow-Up Studies,Humans,Intervertebral Disc Displacement,Male,Middle Aged,Prostheses and Implants,Spinal Fusion,Spinal Stenosis,Titanium,Treatment Outcome},
