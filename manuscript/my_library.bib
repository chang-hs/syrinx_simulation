  pmid = {15658124},
  keywords = {Aged,Decompression; Surgical,Female,Humans,Ligamentum Flavum,Magnetic Resonance Imaging,Male,Middle Aged,Ossification; Heterotopic,Postoperative Care,Preoperative Care,Retrospective Studies,Thoracic Vertebrae,Tomography; X-Ray Computed},
