  shorttitle = {Outcomes and {{Complications Following Laminectomy Alone}} for {{Thoracic Myelopathy}} Due to {{Ossified Ligamentum Flavum}}},
  author = {Osman, Nebiyu S. and Cheung, Zoe B. and Hussain, Awais K. and Phan, Kevin and Arvind, Varun and Vig, Khushdeep S. and Vargas, Luilly and Kim, Jun S. and Cho, Samuel Kang-Wook},
