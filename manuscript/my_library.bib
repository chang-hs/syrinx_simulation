  author = {Heiss, John D. and Jarvis, Katie and Smith, Ren{\'e} K. and Eskioglu, Eric and Gierthmuehlen, Mortimer and Patronas, Nicholas J. and Butman, John A. and Argersinger, Davis P. and Lonser, Russell R. and Oldfield, Edward H.},
