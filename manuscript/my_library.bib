}

@article{matsuzaki2010pilomyxoid,
  title = {Pilomyxoid Astrocytoma of the Cervical Spinal Cord Successfully Treated with Chemotherapy: Case Report},
  shorttitle = {Pilomyxoid Astrocytoma of the Cervical Spinal Cord Successfully Treated with Chemotherapy},
  author = {Matsuzaki, Kazuhito and Kageji, Teruyoshi and Watanabe, Hiroyoshi and Hirose, Takanori and Nagahiro, Shinji},
