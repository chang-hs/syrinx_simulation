  keywords = {Atlanto-Axial Joint,Atlanto-Occipital Joint,Axis; Cervical Vertebra,Cervical Atlas,Endoscopy,Humans,Mouth,Neurosurgical Procedures,Occipital Bone,Pharynx,Skull Base}
}

