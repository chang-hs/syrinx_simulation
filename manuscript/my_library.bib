@article{vidal2019chiari,
  title = {Chiari 1 {{Malformation Surgery}}: {{Comparing Non-violation}} of the {{Arachnoid}} versus {{Arachnoid Opening}} and {{Thermocoagulation}} of the {{Tonsils}}},
