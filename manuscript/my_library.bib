}

@article{liao2005surgical,
  title = {Surgical Experience with Symptomatic Thoracic Ossification of the Ligamentum Flavum},
  author = {Liao, Cheng-Chih and Chen, Tzu-Yung and Jung, Shih-Ming and Chen, Li-Rong},
  year = {2005},
