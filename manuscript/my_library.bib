@article{xia2013prevalence,
  title = {Prevalence of Adjacent Segment Degeneration after Spine Surgery: A Systematic Review and Meta-Analysis},
  shorttitle = {Prevalence of Adjacent Segment Degeneration after Spine Surgery},
  author = {Xia, Xiao-Peng and Chen, Hong-Lin and Cheng, Hong-Bin},
