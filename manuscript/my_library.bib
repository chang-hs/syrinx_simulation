  pmid = {29076793},
  keywords = {Adolescent,Adult,Analysis of Variance,Child,Child; Preschool,Female,Humans,Magnetic Resonance Imaging,Male,Middle Aged,Neural Tube Defects,Observer Variation,Preoperative Care,prone MRI,Prone Position,Retrospective Studies,spine,TCS = tethered cord syndrome,tethered cord,Young Adult},
