  pmid = {13680567},
  keywords = {Adult,Aged,Analysis of Variance,Chronic Disease,Disability Evaluation,Electromyography,Exercise Therapy,Female,Humans,Linear Models,Longitudinal Studies,Low Back Pain,Male,Middle Aged,Muscle Fatigue,Muscle; Skeletal,Treatment Outcome}
