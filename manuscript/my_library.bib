@article{ghasem2017excision,
  title = {Excision of a Centrally Based Ventral Intradural Extramedullary Tumor of the Cervical Spine through a Direct Posterior Approach},
  author = {Ghasem, Alexander and Gjolaj, Joseph P. and Greif, Dylan N. and Green, Barth A.},
