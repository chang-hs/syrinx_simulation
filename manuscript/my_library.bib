  author = {Funao, Haruki and Nakamura, Masaya and Hosogane, Naobumi and Watanabe, Kota and Tsuji, Takashi and Ishii, Ken and Kamata, Michihiro and Toyama, Yoshiaki and Chiba, Kazuhiro and Matsumoto, Morio},
  year = {2012},
