}

@article{riggeal2013clinical,
  title = {Clinical Course of Idiopathic Intracranial Hypertension with Transverse Sinus Stenosis},
  author = {Riggeal, Bryan D. and Bruce, Beau B. and Saindane, Amit M. and Ridha, Maysa A. and Kelly, Linda P. and Newman, Nancy J. and Biousse, Val{\'e}rie},
  year = {2013},
  month = jan,
  journal = {Neurology},
  volume = {80},
  number = {3},
  pages = {289--295},
  issn = {1526-632X},
  doi = {10.1212/WNL.0b013e31827debd6},
  abstract = {OBJECTIVE: Transverse sinus stenosis (TSS) is common in idiopathic intracranial hypertension (IIH), but its effect on the course and outcome of IIH is unknown. We evaluated differences in TSS characteristics between patients with IIH with "good" vs. "poor" clinical courses. METHODS: All patients with IIH seen in our institution after September 2009 who underwent a high-quality standardized brain magnetic resonance venogram (MRV) were included. Patients were categorized as having a good or poor clinical course based on medical record review. The location and percent of each TSS were determined for each patient, and were correlated to the clinical outcome. RESULTS: We included 51 patients. Forty-six patients had bilateral TSS. The median average percent stenosis was 56\%. Seventy-one percent of patients had stenoses {$>$}50\%. Thirty-five of the 51 patients (69\%) had no final visual field loss. Eight patients (16\%) had a clinical course classified as poor. There was no difference in the average percent stenosis between those with good clinical courses vs those with poor courses (62\% vs. 56\%, p = 0.44). There was no difference in the percent stenosis based on the visual field grade (p = 0.38). CSF opening pressure was not associated with either location or degree of TSS. CONCLUSION: TSS is common, if not universal, among patients with IIH, and is almost always bilateral. There is no correlation between the degree of TSS and the clinical course, including visual field loss, among patients with IIH, suggesting that clinical features, not the degree of TSS, should be used to determine management in IIH.},
  langid = {english},
  pmcid = {PMC3589184},
  pmid = {23269597},
  keywords = {Adult,Constriction; Pathologic,Female,Humans,Image Processing; Computer-Assisted,Intracranial Hypertension,Linear Models,Magnetic Resonance Angiography,Magnetic Resonance Imaging,Male,Transverse Sinuses,Treatment Outcome,Vision Disorders,Visual Fields},
