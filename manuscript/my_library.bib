@article{stoodley2000mechanisms,
  title = {Mechanisms Underlying the Formation and Enlargement of Noncommunicating Syringomyelia: Experimental Studies},
  shorttitle = {Mechanisms Underlying the Formation and Enlargement of Noncommunicating Syringomyelia},
  author = {Stoodley, Marcus A. and Jones, Nigel R. and Yang, Liqun and Brown, Christopher J.},
  year = {2000},
