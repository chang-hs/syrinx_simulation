  pmid = {18832664},
  keywords = {Adult,Cerebrospinal Fluid,Female,Humans,Hypothalamus,Image Interpretation; Computer-Assisted,Magnetic Resonance Imaging,Male,Movement,Pulsatile Flow,Young Adult},
