@article{hikata2015impact,
  title = {Impact of Sagittal Spinopelvic Alignment on Clinical Outcomes after Decompression Surgery for Lumbar Spinal Canal Stenosis without Coronal Imbalance},
  author = {Hikata, Tomohiro and Watanabe, Kota and Fujita, Nobuyuki and Iwanami, Akio and Hosogane, Naobumi and Ishii, Ken and Nakamura, Masaya and Toyama, Yoshiaki and Matsumoto, Morio},
  year = {2015},
  month = oct,
  journal = {Journal of Neurosurgery. Spine},
  volume = {23},
  number = {4},
  pages = {451--458},
  issn = {1547-5646},
  doi = {10.3171/2015.1.SPINE14642},
  abstract = {OBJECT: The object of this study was to investigate correlations between sagittal spinopelvic alignment and improvements in clinical and quality-of-life (QOL) outcomes after lumbar decompression surgery for lumbar spinal canal stenosis (LCS) without coronal imbalance. METHODS: The authors retrospectively reviewed data from consecutive patients treated for LCS with decompression surgery in the period from 2009 through 2011. They examined correlations between preoperative or postoperative sagittal vertical axis (SVA) and radiological parameters, clinical outcomes, and health-related (HR)QOL scores in patients divided according to SVA. Clinical outcomes were assessed according to Japanese Orthopaedic Association (JOA) and visual analog scale (VAS) scores. Health-related QOL was evaluated using the Roland-Morris Disability Questionnaire (RMDQ) and the JOA Back Pain Evaluation Questionnaire (JOABPEQ). RESULTS: One hundred nine patients were eligible for inclusion in the study. Compared to patients with normal sagittal alignment prior to surgery (Group A: SVA {$<$} 50 mm), those with preoperative sagittal imbalance (Group B: SVA {$\geq$} 50 mm) had significantly smaller lumbar lordosis and thoracic kyphosis angles and larger pelvic tilt. In Group B, there was a significant decrease in postoperative SVA compared with the preoperative SVA (76.3 {$\pm$} 29.7 mm vs. 54.3 {$\pm$} 39.8 mm, p = 0.004). The patients in Group B with severe preoperative sagittal imbalance (SVA {$>$} 80 mm) had residual sagittal imbalance after surgery (82.8 {$\pm$} 41.6 mm). There were no significant differences in clinical and HRQOL outcomes between Groups A and B. Compared to patients with normal postoperative SVA (Group C: SVA {$<$} 50 mm), patients with a postoperative SVA {$\geq$} 50 mm (Group D) had significantly lower JOABPEQ scores, both preoperative and postoperative, for walking ability (preop: 36.6 {$\pm$} 26.3 vs. 22.7 {$\pm$} 26.0, p = 0.038, respectively; postop: 71.1 {$\pm$} 30.4 vs. 42.5 {$\pm$} 29.6, p {$<$} 0.001) and social functioning (preop: 38.7 {$\pm$} 18.5 vs. 30.2 {$\pm$} 16.7, p = 0.045; postop: 67.0 {$\pm$} 25.8 vs. 49.6 {$\pm$} 20.0, p = 0.001), as well as significantly higher postoperative RMDQ (4.9 {$\pm$} 5.2 vs. 7.9 {$\pm$} 5.7, p = 0.015) and VAS scores for low-back pain (2.68 {$\pm$} 2.69 vs. 3.94 {$\pm$} 2.59, p = 0.039). CONCLUSIONS: Preoperative sagittal balance was not significantly correlated with clinical or HRQOL outcomes after decompression surgery in LCS patients without coronal imbalance. Decompression surgery improved the SVA value in patients with preoperative sagittal imbalance; however, the patients with severe preoperative sagittal imbalance (SVA {$>$} 80 mm) had residual imbalance after decompression surgery. Both clinical and HRQOL outcomes were negatively affected by postoperative residual sagittal imbalance.},
  langid = {english},
  pmid = {26140404},
  keywords = {Adult,Aged,Aged; 80 and over,C7-CSVL = C-7 plumb line deviation from the central sacral vertical line,decompression surgery,Decompression; Surgical,Disability Evaluation,EBL = estimated blood loss,Female,HRQOL = health-related quality of life,Humans,JOA = Japanese Orthopaedic Association,JOABPEQ = JOA Back Pain Evaluation Questionnaire,Laminectomy,LBP = low-back pain,LCS = lumbar spinal canal stenosis,lumbar spinal canal stenosis,Lumbar Vertebrae,Male,MCID = minimum clinically important difference,Middle Aged,Pain Measurement,quality of life,Retrospective Studies,RMDQ = Roland-Morris Disability Questionnaire,sagittal vertical axis,Spinal Curvatures,Spinal Stenosis,spinopelvic alignment,Surveys and Questionnaires,SVA = sagittal vertical axis,Treatment Outcome,VAS = visual analog scale},
  file = {/home/chang/Dropbox/Zotero_pdf/hikata2015impact.pdf}
}

